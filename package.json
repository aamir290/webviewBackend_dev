{
  "name": "searchdirectoryapi",
  "version": "0.1.0",
  "description": "api for search directory interface",
  "main": "./bin/www",
  "scripts": {
    "test": "npm run test-unit",
    "start": "cross-env DEBUG=searchDirectoryApi*,-*debug node ./bin/www",
    "dev": "cross-env DEBUG=searchDirectoryApi* NODE_ENV=development nodemon ./bin/www",
    "test:unit": "cross-env NODE_ENV_=test mocha --opts test/mocha.opts.unit ",
    "coverage": "cross-env NODE_ENV_=test nyc --all mocha --opts test/mocha.opts.unit ",
    "coverage:html": "cross-env NODE_ENV_=test nyc --all --reporter=html mocha --opts test/mocha.opts.unit "
  },
  "repository": {
    "type": "git",
    "url": "https://www.forge.orange-labs.fr/plugins/git/maapservinfra/FrontEnd_BotGallery_anticipation.git"
  },
  "author": "Jeremy Le Piolet <jeremy.le-piolet@gfi.fr>",
  "license": "ISC",
  "dependencies": {
    "cross-env": "^5.2.0",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "http-errors": "~1.6.2",
    "http-status": "^1.2.0",
    "morgan": "~1.9.0",
    "rotating-file-stream": "^1.3.9",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.8.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0"
  },
  "nyc": {
    "exclude": [
      "test",
      "config",
      "public",
      "bin",
      "coverage",
      "app.js"
    ]
  }
}
